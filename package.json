{
  "name": "morten.dev",
  "version": "0.6.0",
  "description": "Source code of morten.dev, my personal website",
  "repository": "https://github.com/Mrtenz/morten.dev",
  "author": "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12"
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "yarn run lint:types && yarn run lint:source && yarn run lint:format",
    "lint:types": "tsc --noEmit",
    "lint:source": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx",
    "lint:format": "prettier --check --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "deploy": "gatsby-plugin-s3 deploy --yes"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@mdx-js/mdx": "^1.6.5",
    "@mdx-js/react": "^1.6.5",
    "@types/mdx-js__react": "^1.5.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby": "^2.24.73",
    "gatsby-plugin-canonical-urls": "^2.3.2",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-google-analytics": "^2.3.3",
    "gatsby-plugin-layout": "^1.3.2",
    "gatsby-plugin-mdx": "^1.2.13",
    "gatsby-plugin-react-helmet-async": "^1.0.17-0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-sitemap": "^2.4.3",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-ts-config": "0.2.3-pre.1",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.8",
    "gatsby-remark-smartypants": "^2.3.3",
    "gatsby-source-filesystem": "^2.3.8",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "rehype-slug": "^3.0.0",
    "remark-unwrap-images": "^2.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.0.2",
    "@reduxjs/toolkit": "^1.4.0",
    "date-fns": "^2.14.0",
    "keccak": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "styled-components": "^5.2.0",
    "typeface-public-sans": "^1.1.4",
    "typeface-source-code-pro": "^1.1.3",
    "typeface-source-serif-pro": "^1.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
