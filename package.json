{
  "name": "morten.dev",
  "version": "0.9.0",
  "description": "Source code of morten.dev, my personal website",
  "repository": "https://github.com/Mrtenz/morten.dev",
  "author": "Maarten Zuidhoorn <maarten@zuidhoorn.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "lint": "yarn run lint:types && yarn run lint:source && yarn run lint:format",
    "lint:types": "tsc --noEmit",
    "lint:source": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js,.jsx",
    "lint:format": "prettier --check --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,yml,md}\"",
    "deploy": "gatsby-plugin-s3 deploy --yes"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.5",
    "@types/mdx-js__react": "^1.5.4",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/refractor": "^3.0.2",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby": "^3.9.1",
    "gatsby-plugin-canonical-urls": "^3.9.0",
    "gatsby-plugin-cname": "^1.0.0",
    "gatsby-plugin-google-analytics": "^3.9.0",
    "gatsby-plugin-layout": "^2.9.0",
    "gatsby-plugin-mdx": "^2.9.0",
    "gatsby-plugin-react-helmet-async": "^1.2.0",
    "gatsby-plugin-react-redux": "^1.1.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-sitemap": "^4.5.0",
    "gatsby-plugin-styled-components": "^4.9.0",
    "gatsby-plugin-ts-config": "1.1.5",
    "gatsby-plugin-typescript": "^3.9.0",
    "gatsby-remark-copy-linked-files": "^4.6.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.6.0",
    "gatsby-remark-smartypants": "^4.6.0",
    "gatsby-source-filesystem": "^3.9.0",
    "husky": "^4.2.5",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "rehype-slug": "^4.0.1",
    "remark-unwrap-images": "^2.1.0",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/providers": "^5.4.1",
    "@reduxjs/toolkit": "^1.6.0",
    "date-fns": "^2.22.1",
    "hmet": "^0.1.2",
    "keccak": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "refractor": "^4.1.0",
    "styled-components": "^5.3.0",
    "typeface-public-sans": "^1.1.13",
    "typeface-source-code-pro": "^1.1.13",
    "typeface-source-serif-pro": "^1.1.13"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
